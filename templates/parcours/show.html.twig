{% extends 'base.html.twig' %}

{% block title %}Parcours{% endblock %}

{% block body %}
    <h1>Parcours</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ parcour.id }}</td>
            </tr>
            <tr>
                <th>TitreParcours</th>
                <td>{{ parcour.titreParcours }}</td>
            </tr>
            <tr>
                <th>SousTitreParcours</th>
                <td>{{ parcour.sousTitreParcours }}</td>
            </tr>
            <tr>
                <th>DateDebutParcours</th>
